{
  "name": "tinker-vue",
  "version": "0.1.364",
  "description": "secret",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "bump": "bumpp patch --commit --tag --push --yes",
    "deploy-app": "cd dist; ln -s ../.now .vercel; vc --prod",
    "release": "pnpm bump && pnpm build && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "dev": "concurrently \"pnpm:twx --watch\" \"pnpm:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "build": "pnpm twx; pnpm format; vite build",
    "serve": "vite preview",
    "test": "echo skipping...",
    "cc": "prettier --check './src/**/*.{css,pcss,js,vue}'",
    "format": "prettier --write './src/**/*.{css,pcss,js,vue}'",
    "spell": "cspell './**/*.{js,vue}'",
    "open": "open https://tinker-vue.vercel.app"
  },
  "dependencies": {
    "core-js": "3.48.0",
    "pinia": "3.0.4",
    "vue": "3.5.27"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "6.0.3",
    "autoprefixer": "10.4.24",
    "bumpp": "10.4.0",
    "concurrently": "9.2.1",
    "cspell": "9.6.2",
    "only-allow": "1.2.2",
    "postcss": "8.5.6",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "proto-tailwindcss-clrs": "0.0.464",
    "tailwindcss": "4.1.18",
    "vite": "7.3.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
