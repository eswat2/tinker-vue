{
  "name": "tinker-vue",
  "version": "0.1.320",
  "description": "secret",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "concurrently \"yarn:twx --watch\" \"yarn:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "build": "yarn twx; yarn format; vite build",
    "serve": "vite preview",
    "test": "echo skipping...",
    "cc": "prettier --check './src/**/*.{css,pcss,js,vue}'",
    "format": "prettier --write './src/**/*.{css,pcss,js,vue}'",
    "spell": "cspell './**/*.{js,vue}'",
    "predeploy": "yarn version --patch; yarn build",
    "deploy": "cd dist; ln -s ../.now .vercel; vc --prod",
    "open": "open https://tinker-vue.vercel.app"
  },
  "dependencies": {
    "core-js": "3.45.1",
    "pinia": "3.0.3",
    "vue": "3.5.22"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "6.0.1",
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.1",
    "cspell": "9.2.1",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "proto-tailwindcss-clrs": "0.0.446",
    "tailwindcss": "4.1.14",
    "vite": "7.1.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
