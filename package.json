{
  "name": "tinker-vue",
  "version": "0.1.17",
  "description": "secret",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"yarn:twx --watch\" \"yarn:start\"",
    "twx": "sh ./.generate",
    "start": "vite",
    "build": "yarn twx; yarn format; vite build",
    "serve": "vite preview",
    "test": "echo skipping...",
    "cc": "prettier --check './src/**/*.{css,pcss,js,vue}'",
    "format": "prettier --write './src/**/*.{css,pcss,js,vue}'",
    "spell": "cspell './**/*.{js,vue}'",
    "predeploy": "yarn version --patch; yarn build",
    "deploy": "cd dist; ln -s ../.now .vercel; vc --prod",
    "open": "open https://tinker-vue.vercel.app"
  },
  "dependencies": {
    "core-js": "3.35.1",
    "pinia": "2.1.7",
    "vue": "3.4.18"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.0.3",
    "autoprefixer": "10.4.17",
    "concurrently": "8.2.2",
    "cspell": "8.3.2",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "proto-tailwindcss-clrs": "0.0.308",
    "tailwindcss": "3.4.1",
    "vite": "5.1.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
